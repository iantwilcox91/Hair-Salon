<h1>Stylist</h1>
@if (@Model.Count == 0)
  {
    <p>No Stylists Added</p>
  }
else
{
  <ol>
    @foreach (var Stylist in Model)
    {
      <a href="/viewTheClientsForThisStylist/{id}"><li>@Stylist.GetName()</li></a>
    }
  </ol>
}
<p><a href="/addNewClientToStylist">Add a Client</a></p>
<p><a href="/deleteAllStylists">Delete all Stylists</a></p>
<p><a href="/">Home</a></p>
